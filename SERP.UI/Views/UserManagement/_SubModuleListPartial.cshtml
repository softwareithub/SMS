@model IEnumerable<SERP.Core.Model.UserManagement.UserAccessRightModel>


<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-12">


                    <table id="tblDataList" class="table table-striped table-bordered responsive" style="width:100%">
                        <thead>
                            <tr>
                                <th>Module Name</th>
                                <th>SubModule Name</th>
                                <th>Controller Name</th>
                                <th>Action Name</th>
                                <th>Class Name</th>
                                <th>Display Order</th>
                                <th>Action</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var data in Model)
                            {
                            <tr>
                                <td>
                                    @data.ModuleName
                                </td>
                                <td>
                                    @data.SubModuleName
                                </td>
                                <td>
                                    @data.ControllerName
                                </td>
                                <td>
                                    @data.ActionName
                                </td>
                                <td>
                                    @data.ClassName
                                </td>
                                <td>
                                    @data.DisplayOrder
                                </td>


                                <td>
                                    <a class="btn btn-outline-success pull-right" data-loading-text="<i class='fa fa-circle-o-notch fa-spin'  style='color:#DD4B39'></i>" onclick="UpdateRecord(@data.SubModuleId, this)">
                                        <i class="fa fa-edit" style="color:#3C8DBC"></i>

                                    </a>

                                    <a class="btn btn-outline-danger pull-right" data-loading-text="<i class='fa fa-circle-o-notch fa-spin'  style='color:#DD4B39'></i>" onclick="DeleteRecord(@data.SubModuleId, this)">
                                        <i class="fa fa-trash" style="color:#DD4B39"></i>
                                    </a>
                                </td>
                            </tr>
                            }
                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
<script type="text/javascript">
    function DeleteRecord(id, event) {
        var geturl = "/SubMenu/SubModuleList";
        var deleteUrl = "/SubMenu/Delete";
        CustomDeleteRecord(id, geturl, deleteUrl, event);
    }
    function UpdateRecord(id, event) {
        var updateUrl = "/SubMenu/Index";
        UpdateCustomRecord(id, updateUrl, event);
    }
</script>

