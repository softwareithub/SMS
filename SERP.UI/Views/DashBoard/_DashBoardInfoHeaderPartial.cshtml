@model SERP.Core.Model.DashBoardModel.DashBoardDetailVm
<div class="col-lg-3 col-xs-6">
    <!-- small box -->
    <div class="small-box bg-aqua">
        <div class="inner">
            <h3>@Model.StudentCount</h3>

            <p>Students Details</p>
        </div>
        <div class="icon">
            <i class="fa fa-user"></i>
        </div>
        <a  data-ajax="true" data-ajax-mode="replace" 
               data-ajax-update="#divSERP"
                                   data-ajax-begin="AjaxOnBegin" data-ajax-complete="AjaxComplete"
           asp-action="CreateStudent" asp-controller="StudentMaster" class="small-box-footer">More info 
        <i class="fa fa-arrow-circle-right"></i></a>
    </div>
</div>

