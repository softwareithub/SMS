@model IEnumerable<SERP.Core.Model.TransactionViewModel.FreeEmployeeModel>

    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-12">


                        <table id="tblDataList" class="table table-striped table-bordered responsive" style="width:100%">
                            <thead>
                                <tr>
                                    <th>Employee Name</th>
                                    <th>Employee Image</th>
                                    <th>Action</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var data in Model)
                                {
                                    <tr>
                                        <td>
                                            @data.EmployeeName
                                        </td>
                                        <td>
                                            @Html.SerpImageHelper(data.EmployeePhoto,75,75,"employeeId")
                                        </td>
                                        <td>
                                            <a class="btn btn-outline-success pull-right" data-loading-text="<i class='fa fa-circle-o-notch fa-spin'  style='color:#DD4B39'></i>" onclick="UpdateRecord()">
                                                <i class="fa fa-edit" style="color:#3C8DBC"></i>

                                            </a>
                                        </td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>

        </div>
    </section>
